        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DeviseActivity"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MajNomDeviseActivity"
            android:label="@string/title_activity_maj_nom_devise"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".content_main"
            android:label="@string/title_activity_content_main"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>